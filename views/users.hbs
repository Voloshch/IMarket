<h1 style="margin-left: 450px;margin-top: 50px">Управление пользователями</h1>
<table class="table" style="width: 900px; margin-left:300px; margin-top:100px">
    <thead>
    <tr>
        <th>id</th>
        <th>Email</th>
        <th>Имя</th>
        <th>Фамилия</th>
        <th>Телефон</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    {{#each user}}
    <tr>
        <th>{{id}}</th>
        <td>{{email}}</td>
        <th>{{name}}</th>
        <td>{{lastname}}</td>
        <td>{{phone}}</td>
        <td><a class="btn btn-primary" href="/deleteuser/{{id}}">
            Удалить пользователя
        </a></td>
    </tr>
    {{/each}}
    </tbody>
</table>
